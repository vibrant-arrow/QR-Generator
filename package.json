{
  "name": "qr-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": {
    "name": "Vem Aiensi",
    "email": "vem.aiensi@gmail.com"
  },
  "description": "an offline text-to-qr tool",
  "main": "./dist-electron/main.js",
  "scripts": {
    "dev": "run-p dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "pnpm run transpile:electron && cross-env NODE_ENV=development electron .",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "dist:win": "pnpm run build && pnpm run transpile:electron && electron-builder --win --x64",
    "dist:linux": "pnpm run build && pnpm run transpile:electron && electron-builder --linux --x64",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "cross-env": "^7.0.3",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
